    "import numpy as np\n",
    "\n",
    "arr1 = np.array([1.1, 1.2, 1.3, 1.4, 1.5])\n",
    "arr2 = np.arange(4)\n",
    "condicional = np.array([True, False, True, True, False])\n",
    "\n",
    "arr2_resized = np.resize(arr2, condicional.shape)\n",
    "\n",
    "resultado = np.where(condicional, arr1, arr2_resized)\n",
    "\n",
    "print(\"Resultado:\", resultado)"
   ]
